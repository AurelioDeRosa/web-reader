{
   "bitwise": true,
   "curly": true,
   "eqeqeq": true,
   "esversion": 6,
   "forin": false,
   "freeze": true,
   "funcscope": false,
   "globals": [],
   "iterator": false,
   "latedef": true,
   "maxcomplexity": 6,
   "maxdepth": 3,
   "maxerr": 50,
   "maxparams": 4,
   "maxstatements": 20,
   "noarg": true,
   "nocomma": true,
   "nonbsp": true,
   "nonew": true,
   "notypeof": true,
   "predef": [
      "fixture",
      "assert"
   ],
   "shadow": "inner",
   "singleGroups": true,
   "strict": "implied",
   "undef": true,
   "unused": true,
   "varstmt": false,

   "browser": true,
   "devel": true,
   "mocha": true
}